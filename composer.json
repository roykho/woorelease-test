{
    "name": "woo/woorelease-test",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "archive": {
        "exclude": [
            "!/dist",
            "/languages",
            "bin",
            "tests",
            "/vendor",
            "Gruntfile.js",
            "node_modules",
            "README.md",
            "package.json",
            "package-lock.json",
            "composer.json",
            "composer.lock",
            "phpunit.xml.dist",
            "woocommerce-shipping-fedex.zip",
            "includes/box-packer/vendor/dvdoug/boxpacker",
            "!includes/box-packer/vendor/dvdoug/boxpacker/features",
            "!includes/box-packer/vendor/dvdoug/boxpacker/src",
            ".*"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
